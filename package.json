    "states:discover": "node scripts/discover-states.mjs",
    "states:export": "node scripts/export-states.mjs",
    "states:normalize": "node scripts/tools/normalize-states.mjs",
    "audit:refs": "node scripts/audit-refs.mjs",
    "test": "playwright test",
    "test:e2e": "playwright test tests/e2e",
    "test:contracts": "playwright test tests/contracts",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report"
